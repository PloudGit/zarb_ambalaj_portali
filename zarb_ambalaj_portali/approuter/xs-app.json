{
    "welcomeFile": "/index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/sap/(.*)$",
            "target": "/sap/$1",
            // "destination": "aribads4connection",
            "destination": "aribaqs4connection",
            "authenticationType": "xsuaa",
            "scope": [
                "$XSAPPNAME.Supplier",
                "$XSAPPNAME.Admin"
            ],
            "csrfProtection": false
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "localDir": "webapp",
            "authenticationType": "xsuaa",
            "scope": [
                "$XSAPPNAME.Supplier",
                "$XSAPPNAME.Admin"
            ]
        }
    ]
}